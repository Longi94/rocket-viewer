{
  "name": "rocket-viewer",
  "version": "0.1.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run wasm-pack && ng serve",
    "start:connect": "npm run wasm-pack && ng serve --host 0.0.0.0",
    "build": "npm run wasm-pack && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "wasm-pack": "npm run wasm-pack:boxcars && npm run wasm-pack:copy",
    "wasm-pack:copy": "cpx boxcars-wasm/pkg/*.{js,ts,wasm} src/app/boxcars/",
    "wasm-pack:boxcars": "cd boxcars-wasm && wasm-pack build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "color-hash": "^1.0.3",
    "ng5-slider": "^1.2.4",
    "rl-loadout-lib": "^0.6.0",
    "rxjs": "~6.5.4",
    "stats.js": "^0.17.0",
    "three": "^0.114.0",
    "three-nebula": "^5.1.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.0.0-beta.1",
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "circular-dependency-plugin": "^5.2.0",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
